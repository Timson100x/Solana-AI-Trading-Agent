{
  "name": "solana-ai-trading-agent-pro",
  "version": "2.1.0",
  "description": "Professional AI Trading Bot with ElizaOS Plugin V2",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "node scripts/test.js",
    "health": "node tests/system-health-check.js",
    "enhanced": "node -e \"import('./src/enhanced-bot.js').then(m => m.default.run())\"",
    "scanner": "node src/scheduler.js",
    "test-auto": "node scripts/test-auto-trading.js",
    "test-tricks": "node scripts/test-pro-tricks.js",
    "phase2": "node index.js",
    "godmode": "GODMODE=true node index.js",
    "deploy": "node src/system/final-deploy.js",
    "deploy-live": "node src/system/final-deploy.js --enable-live-trading",
    "audit": "node src/system/final-deploy.js",
    "optimize": "node -e \"import('./src/services/wallet-optimizer.js').then(m => {const w = new m.WalletOptimizer(); w.optimize(0.17);})\"",
    "monitor": "node -e \"import('./src/monitoring/position-monitor.js').then(m => new m.PositionMonitor().start())\"",
    "webhook": "node -e \"import('./src/services/helius-webhooks.js').then(m => new m.HeliusWebhooks().startServer(3000))\"",
    "backup": "node -e \"import('./src/utils/backup-manager.js').then(m => new m.BackupManager().createBackup())\"",
    "restore": "node -e \"import('./src/utils/backup-manager.js').then(m => new m.BackupManager().listBackups().then(console.log))\"",
    "setup": "npm install && node scripts/setup.js"
  },
  "keywords": [
    "solana",
    "trading",
    "ai",
    "jupiter",
    "elizaos",
    "day-trading",
    "defi",
    "memecoin"
  ],
  "author": "Joko588",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@elizaos/plugin-solana": "^1.2.6",
    "@google/generative-ai": "^0.21.0",
    "@jup-ag/api": "^6.0.28",
    "@quicknode/sdk": "^2.5.2",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.0",
    "axios": "^1.7.7",
    "bignumber.js": "^9.1.2",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.5",
    "es-abstract": "^1.24.1",
    "eventemitter3": "^5.0.1",
    "express": "^4.19.2",
    "groq-sdk": "^0.3.0",
    "helius-sdk": "^2.0.5",
    "jito-ts": "^4.2.1",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^7.0.12",
    "oauth-1.0a": "^2.2.6",
    "qrcode": "^1.5.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.6"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
